#!/bin/bash
# logparser - Parse Mu2e log performance metrics
# Usage: logparser dataset.art [dataset2.art ...] [options]

# Get the directory where this script is located
SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
PRODTOOLS_DIR="$(dirname "$SCRIPT_DIR")"

# Run the Python script
exec python3 "$PRODTOOLS_DIR/utils/logparser.py" "$@"


